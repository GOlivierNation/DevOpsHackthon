{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  },
  "functions": {
    "app/api/health/route.ts": {
      "maxDuration": 10
    },
    "app/api/metrics/route.ts": {
      "maxDuration": 15
    },
    "app/api/pipeline/runs/route.ts": {
      "maxDuration": 20
    },
