{
  "name": "devops-hackathon-pipeline",
  "version": "1.2.3",
  "description": "Complete DevOps CI/CD pipeline demonstration for hackathon",
  "keywords": [
    "devops",
    "hackathon",
    "ci-cd",
    "docker",
    "kubernetes",
    "monitoring",
    "prometheus",
    "grafana",
    "github-actions",
    "aws",
    "eks"
  ],
  "homepage": "https://github.com/GOlivierNation/DevOpsHackthon#readme",
  "bugs": {
    "url": "https://github.com/GOlivierNation/DevOpsHackthon/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GOlivierNation/DevOpsHackthon.git"
  },
  "license": "MIT",
  "author": "GOlivierNation",
  "main": "index.js",
  "scripts": {
    "build": "next build",
    "deploy:railway": "railway up",
    "deploy:vercel": "vercel --prod",
    "dev": "next dev",
    "docker:build": "docker build -t devops-pipeline-app .",
    "docker:run": "docker run -p 3000:3000 devops-pipeline-app",
    "k8s:delete": "kubectl delete -f k8s/",
    "k8s:deploy": "kubectl apply -f k8s/",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "security:audit": "npm audit --audit-level high",
    "security:fix": "npm audit fix",
    "setup:local": "chmod +x scripts/setup.sh && ./scripts/setup.sh",
    "start": "next start",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.454.0",
    "next": "14.2.16",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "10.4.16",
    "eslint": "8.55.0",
    "eslint-config-next": "14.0.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5",
    "tailwindcss": "3.3.6",
    "typescript": "^5"
  }
}